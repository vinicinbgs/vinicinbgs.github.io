{
  "private": true,
  "name": "minimal-blog",
  "description": "Typography driven, feature-rich blogging theme with minimal aesthetics. Includes tags/categories support and extensive features for code blocks such as live preview, line numbers, and line highlighting.",
  "version": "1.3.12",
  "author": "LekoArts <hello@lekoarts.de>",
  "license": "0BSD",
  "starter-name": "gatsby-starter-minimal-blog",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "develop:cypress": "cross-env CYPRESS_SUPPORT=y yarn develop",
    "build:cypress": "cross-env CYPRESS_SUPPORT=y yarn build",
    "start": "gatsby develop -p 9001",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "sh deploy.sh",
    "test": "jest",
    "test:text": "yarn test --coverage --coverageReporters='text-summary'",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:headless": "cypress run --browser=chrome --headless",
    "cy:start:run": "yarn start-server-and-test start http://localhost:9001 cy:run",
    "cy:report:html": "yarn nyc report --reporter=html",
    "cy:report:text": "yarn cy:run:headless && yarn nyc report --reporter=text",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@lekoarts/gatsby-theme-minimal-blog": "^4.0.0",
    "@react-icons/all-files": "^4.1.0",
    "gatsby": "^4.4.0",
    "gatsby-image": "^3.11.0",
    "gatsby-omni-font-loader": "^1.3.1",
    "gatsby-plugin-feed": "^4.0.0",
    "gatsby-plugin-gatsby-cloud": "^4.0.0",
    "gatsby-plugin-google-gtag": "^4.9.0",
    "gatsby-plugin-manifest": "^4.0.0",
    "gatsby-plugin-sharp": "^4.7.0",
    "gatsby-plugin-sitemap": "^5.0.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.22",
    "gatsby-source-filesystem": "^4.7.0",
    "gatsby-transformer-sharp": "^4.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.4",
    "@babel/plugin-syntax-jsx": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-flow": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/instrument-cra": "^1.4.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@react-theming/storybook-addon": "^1.1.5",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/react-icons": "^3.0.0",
    "@types/styled-components": "^5.1.22",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "axe-core": "^4.4.1",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-istanbul": "6.0.0",
    "babel-plugin-remove-graphql-queries": "^4.7.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-gatsby": "^2.7.0",
    "babel-preset-react-app": "^10.0.1",
    "cross-env": "^7.0.3",
    "cypress": "^9.4.1",
    "cypress-axe": "^0.14.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.6",
    "gh-pages": "^3.2.3",
    "jest": "^27.5.1",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "react-test-renderer": "^17.0.2",
    "start-server-and-test": "^1.14.0",
    "storybook-addon-gatsby": "^0.0.5",
    "typescript": "^4.5.5"
  }
}
